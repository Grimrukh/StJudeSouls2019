@RestartOnRest
def AllyStJude(ally: Character):
    """ 11212000: Ally gets St Jude buff. """
    await HasSpecialEffect(PLAYER, SPEFFECT.RingOfStJude)
    AddSpecialEffect(ally, SPEFFECT.RingOfStJudeAlly)
    await (not HasSpecialEffect(PLAYER, SPEFFECT.RingOfStJude))
    CancelSpecialEffect(ally, SPEFFECT.RingOfStJudeAlly)
    return RESTART


@RestartOnRest
def AllyMustRecover(ally: Character):
    """ 11212010: Ally must recover health when they take enough damage. (Not for Sif.) """
    await (IsAlive(CHR.FramptBoss) and HasSpecialEffect(ally, SPEFFECT.RingOfStJudeAlly))
    EnableImmortality(ally)
    IsWeak = Condition(HealthRatio(ally) <= 0.1, hold=True)
    IsOffCliff = Condition(EntityInsideRegion(ally, 1212690), hold=True)
    await (IsWeak or IsOffCliff)
    if IsOffCliff:
        WarpToEntity(ally, 1212691, destination_type=Category.region, short_warp=True)
    AddSpecialEffect(ally, 3231)
    DisableCharacter(ally)
    Wait(30.0)
    EnableCharacter(ally)
    return RESTART


@RestartOnRest
def SifImmortality():
    """ 11212020: Sif version of recovery. """
    # Sif is just invincible. Checks for ring every ten seconds.
    await HasSpecialEffect(CHR.Sif, SPEFFECT.RingOfStJudeAlly)
    EnableInvincibility(CHR.Sif)
    DisableHealthBar(CHR.Sif)
    Wait(10.0)
    return RESTART


def SifOffCliff():
    """ 11212021: Sif falls. """
    await (HasSpecialEffect(CHR.Sif, SPEFFECT.RingOfStJudeAlly) and EntityInsideRegion(CHR.Sif, 1212690))
    WarpToEntity(CHR.Sif, 1212691, destination_type=Category.region, short_warp=True)
    DisableCharacter(CHR.Sif)
    Wait(10.0)
    EnableCharacter(CHR.Sif)
    return RESTART
