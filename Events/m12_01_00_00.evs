    await IsDead(soldier_id)
    return


@RestartOnRest
def FullBattleSequence():
    """ 11210000: Black Eye Gough spawns. """
    if FLAG.BattleStarted:
        # Game was save-and-quitted. Return to Firelink.
        DisableFlag(FLAG.BattleStarted)
        WarpToMap(FIRELINK_SHRINE)
        return

    EnableFlag(FLAG.BattleStarted)

    DisableCharacter(CHR.BlackEyeGough)

    SetTeamType(CHR.FramptSwoop, TeamType.white_phantom)  # So allies don't try to attack him.
    DisableAI(CHR.FramptSwoop)
    EnableImmortality(CHR.FramptSwoop)
    DisableGravity(CHR.FramptSwoop)
    DisableCollision(CHR.FramptSwoop)
    DisableCharacter(CHR.FramptSwoop)
    SetNetworkUpdateAuthority(CHR.FramptSwoop, UpdateAuthority.forced)
    SetNetworkUpdateRate(CHR.FramptSwoop, is_fixed=1, frequency=CharacterUpdateRate.always)

    DisableCharacter(CHR.FramptBoss)

    # PHASE 1: Enemy Soldiers

    # PHASE 2: Black-Eye Gough

    IfTrueFlagCountGreaterThanOrEqual(0, 30, 11215400, 11215442)  # Thirty enemies dead.

    # Frampt swoops overhead.

    EnableCharacter(CHR.FramptSwoop)
    ForceAnimation(CHR.FramptSwoop, 7006, loop=False, wait_for_completion=False, skip_transition=True)
    WaitFrames(240)
    EnableCharacter(CHR.BlackEyeGough)
    EnableInvincibility(CHR.BlackEyeGough)
    ForceAnimation(CHR.FramptSwoop, 7008, loop=False, wait_for_completion=False, skip_transition=True)
    WaitFrames(194)
    DisableInvincibility(CHR.BlackEyeGough)
    EnableBossHealthBar(CHR.BlackEyeGough, TEXT.GoughName)

    await IsDead(CHR.BlackEyeGough)

    DisableBossHealthBar(CHR.BlackEyeGough)

    Wait(3.0)

    # PHASE 3: Frampt, the Primordial Serpent-God

    EnableMapSound(1213803)
    EnableCharacter(CHR.FramptBoss)
    EnableBossHealthBar(CHR.FramptBoss, name_id=4510)

    await (HealthRatio(CHR.FramptBoss) <= 0.0)

    Wait(2.0)
    PlaySoundEffect(PLAYER, SoundType.v_voice, 215000006)

    await IsDead(CHR.FramptBoss)

    DisableBossHealthBar(CHR.FramptBoss)
    DisableMapSound(1213803)
    PlaySoundEffect(CHR.FramptBoss, SoundType.s_sfx, 777777777)

    # If you weren't wearing the Ring of St. Jude (you masochist), you get a message and go back to Firelink.

    if IsDead(CHR.Patches):
        DisplayStatus(TEXT.HollowVictory)
        Wait(5.0)
        WarpToMap(FIRELINK_SHRINE)
        return

    # PHASE 4: Trusty Patches

    DisplayBanner(TextBannerType.boss_defeated)
    EnableImmortality(PLAYER)

    # No more recovery.
    StopEvent(11212010, slot=0)
    StopEvent(11212010, slot=1)
    StopEvent(11212010, slot=2)
    StopEvent(11212010, slot=3)
    StopEvent(11212010, slot=4)
    StopEvent(11212010, slot=5)
    StopEvent(11212010, slot=6)
    StopEvent(11212020)  # Sif.

    EnableCharacter(CHR.Solaire)
    EnableCharacter(CHR.Siegmeyer)
    EnableCharacter(CHR.Logan)
    EnableCharacter(CHR.Ornstein)
    EnableCharacter(CHR.Havel)
    EnableCharacter(CHR.Quelana)
    EnableCharacter(CHR.Patches)
    EnableCharacter(CHR.Sif)

    SetNest(CHR.Solaire, 1202680)
    SetNest(CHR.Siegmeyer, 1202681)
    SetNest(CHR.Logan, 1202682)
    SetNest(CHR.Ornstein, 1202683)
    SetNest(CHR.Havel, 1202684)
    SetNest(CHR.Quelana, 1202685)
    SetNest(CHR.Patches, 1202686)  # Most central position.
    SetNest(CHR.Sif, 1202687)

    await (EntityWithinDistance(CHR.Solaire, CHR.Patches, 15.0)
           and EntityWithinDistance(CHR.Siegmeyer, CHR.Patches, 15.0)
           and EntityWithinDistance(CHR.Logan, CHR.Patches, 15.0)
           and EntityWithinDistance(CHR.Quelana, CHR.Patches, 15.0)
           and EntityWithinDistance(CHR.Havel, CHR.Patches, 15.0)
           and EntityWithinDistance(CHR.Sif, CHR.Patches, 15.0)
           # and EntityWithinDistance(CHR.Ornstein, CHR.Patches, 15.0)  # Glitchy AI.
           and EntityWithinDistance(PLAYER, CHR.Patches, 15.0))

    DisableImmortality(CHR.Solaire)
    DisableImmortality(CHR.Siegmeyer)
    DisableImmortality(CHR.Logan)
    DisableImmortality(CHR.Ornstein)
    DisableImmortality(CHR.Havel)
    DisableImmortality(CHR.Quelana)
    DisableImmortality(CHR.Patches)
    # Sif stays immortal.

    Wait(2.0)
    EnableMapSound(1213801)
    Wait(2.0)

    CreateTemporaryFX(15398, anchor_entity=CHR.Patches, model_point=1)
    PlaySoundEffect(anchor_entity=CHR.Patches, sound_type=SoundType.s_sfx, sound_id=21199)
    Kill(CHR.Solaire)
    WaitFrames(5)
    Kill(CHR.Siegmeyer)
    WaitFrames(2)
    Kill(CHR.Ornstein)
    WaitFrames(10)
    Kill(CHR.Quelana)
    WaitFrames(5)
    Kill(CHR.Logan)
    WaitFrames(8)
    Kill(CHR.Havel)

    AddSpecialEffect(CHR.Patches, SPEFFECT.PatchesBuff)
    AddSpecialEffect(CHR.Patches, 3231)  # Patches recovers.
    AddSpecialEffect(CHR.Sif, 3231)  # Sif recovers.
    AddSpecialEffect(CHR.Sif, 5401)  # Sif starts limping.
    StopEvent(11212000, slot=2)  # No more regen for Patches.
    CancelSpecialEffect(CHR.Patches, SPEFFECT.RingOfStJudeAlly)
    SetTeamType(CHR.Patches, TeamType.boss)
    EnableBossHealthBar(CHR.Patches, TEXT.PatchesName)

    await (HealthRatio(CHR.Patches) <= 0.8)

    DisableImmortality(CHR.Sif)
    CreateTemporaryFX(15398, anchor_entity=CHR.Patches, model_point=10)
    PlaySoundEffect(anchor_entity=CHR.Patches, sound_type=SoundType.s_sfx, sound_id=21199)
    Kill(CHR.Sif)

    Wait(5.0)
    DisplayStatus(TEXT.RingBurns)

    # Go crazy on Patches.

    # AddSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong1)
    # await (HealthRatio(CHR.Patches) <= 0.8)
    # CancelSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong1)
    # AddSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong2)
    # await (HealthRatio(CHR.Patches) <= 0.7)
    # CancelSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong2)
    AddSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong3)
    await (HealthRatio(CHR.Patches) <= 0.7)
    CancelSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong3)
    AddSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong4)
    await (HealthRatio(CHR.Patches) <= 0.5)
    CancelSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong4)
    AddSpecialEffect(PLAYER, SPEFFECT.RingOfStJudeStrong5)

    await IsDead(CHR.Patches)

    DisableMapSound(1213801)

    AwardItemLot(ITEMLOT.SerpentSoul)
    EnableFlag(FLAG.AlliesKilled)
    EnableFlag(FLAG.PatchesDead)
    Wait(1.0)  # Can't give player time to use Serpent Soul.
    WarpToMap(FIRELINK_SHRINE)


