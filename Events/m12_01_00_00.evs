@RestartOnRest
def SifLimping():
    """ 11215043: Sif starts limping at 30% health, but recovers at 50%. """
    await (HealthRatio(CHR.Sif) <= 0.3)
    AddSpecialEffect(CHR.Sif, 5401)
    await (HealthRatio(CHR.Sif) >= 0.5)
    CancelSpecialEffect(CHR.Sif, 5401)
    return RESTART


